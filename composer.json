{
    "name": "icecave/chrono-doctrine",
    "description": "Custom Doctrine types for Chrono dates and times.",
    "keywords": [
        "doctrine",
        "chrono",
        "date",
        "time"
    ],
    "homepage": "https://github.com/icecave/chrono-doctrine",
    "license": "MIT",
    "authors": [
        {
            "name": "Erin Millard",
            "email": "ezzatron@gmail.com",
            "homepage": "http://ezzatron.com"
        },
        {
            "name": "James Harris",
            "email": "mailjamesharris@gmail.com",
            "homepage": "https://github.com/jmalloc"
        }
    ],
    "require": {
        "php": ">=7.2",
        "doctrine/dbal": "^2",
        "icecave/chrono": "^2"
    },
    "require-dev": {
        "eloquent/liberator": "~2",
        "friendsofphp/php-cs-fixer": "^2",
        "phake/phake": "^3",
        "phpunit/phpunit": "^8"
    },
    "autoload": { "psr-4": { "Icecave\\Chrono\\Doctrine\\": "src" } },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.2"
        }
    }
}
